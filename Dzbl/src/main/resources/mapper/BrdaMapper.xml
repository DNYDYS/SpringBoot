<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bsoft.mapper.BrdaMapper">

    <select id="getBrda" resultType="com.bsoft.entity.Emr_Brda">
        select *  from Emr_Brda  where patientIndex = #{patientIndex,jdbcType=INTEGER}
    </select>

</mapper>